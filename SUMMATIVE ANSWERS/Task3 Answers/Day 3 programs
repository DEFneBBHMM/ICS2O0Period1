n=int(input("Enter number:"))
fact=1
if n < 0:
  print("n*n")
while(n>0):
    fact=fact*n
    n=n-1
print("Factorial of the number is: ")
print(fact)
f= open("Factorial Results.txt","w+") #opening a file 
for i in range(10):
     f.write("This is line %d\r\n" % (i+1))
f=open("Factorial Results.txt", "a+")
for i in range(2):
     f.write("Appended line %d\r\n" % (i+1))
